{
    "icon_theme": "Colored Zed Icons Theme Dark",
    "show_edit_predictions": false,
    "features": {
        "edit_prediction_provider": "zed"
    },
    "vim_mode": true,
    "theme": "Vercel Dark (Blurred)",
    "ui_font_family": "Menlo",
    "buffer_font_family": "Menlo",
    "ui_font_size": 18,
    "buffer_font_size": 18,
    "buffer_font_features": {
        "calt": false
    },
    "cursor_blink": false,
    "scrollbar": {
        "show": "never"
    },
    "toolbar": {
        "breadcrumbs": true,
        "quick_actions": false,
        "selections_menu": false
    },
    "outline_panel": {
        "button": false
    },
    "relative_line_numbers": true,
    "tab_size": 4,
    "inlay_hints": {
        "enabled": false
    },
    "file_finder": {
        "modal_max_width": "large"
    },
    "tab_bar": {
        "show": false
    },
    "project_panel": {
        "dock": "right",
        "auto_fold_dirs": false
    },
    "format_on_save": "on",
    "diagnostics": {
        "include_warnings": true
    },
    "debugger": {
        "dock": "left"
    }
}
