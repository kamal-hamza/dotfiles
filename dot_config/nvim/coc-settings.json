{
  "suggest.enablePreview": false,
  "suggest.enablePreselect": false,
  "suggest.detailField": "menu",
  "suggest.snippetIndicator": "",
  "suggest.maxCompleteItemCount": 15,
  "suggest.noselect": true,
  "suggest.acceptSuggestionOnCommitCharacter": false,
  "suggest.insertMode": "insert",
  "suggest.labelMaxLength": 60,
  "suggest.formatItems": ["kind", "abbr", "menu"],
  "suggest.selection": "recentlyUsed",
  "suggest.completionItemKindLabels": {
    "text": "󰉿",
    "method": "󰆧",
    "function": "󰊕",
    "constructor": "",
    "field": "󰜢",
    "variable": "󰀫",
    "class": "󰠱",
    "interface": "",
    "module": "",
    "property": "󰜢",
    "unit": "󰑭",
    "value": "󰎠",
    "enum": "",
    "keyword": "󰌋",
    "snippet": "",
    "color": "󰏘",
    "file": "󰈙",
    "reference": "󰈇",
    "folder": "󰉋",
    "enumMember": "",
    "constant": "󰏿",
    "struct": "󰙅",
    "event": "",
    "operator": "󰆕",
    "typeParameter": ""
  },
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "󰠠",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.messageTarget": "float",
  "diagnostic.messageDelay": 250,
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.formatOnSaveFiletypes": [
    "typescript",
    "javascript",
    "typescriptreact",
    "javascriptreact",
    "json",
    "html",
    "css"
  ],
  "snippets.ultisnips.enable": false,
  "snippets.ultisnips.pythonPrompt": false,
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescriptreact": ["typescript"]
  },
  "languageserver": {
    "pyrefly": {
      "command": "pyrefly",
      "args": ["lsp"],
      "filetypes": ["python"],
      "rootPatterns": ["pyrefly.toml", "pyproject.toml", ".git"]
    },
    "csharp-ls": {
      "command": "csharp-ls",
      "filetypes": ["cs"],
      "rootPatterns": ["*.csproj", ".vim/", ".git/", ".hg/"]
    }
  }
}
